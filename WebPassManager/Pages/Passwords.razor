@page "/passwords"
@using EDataAccessLibrary
@using EDataAccessLibrary.Models
@using Models
@using MudBlazor
@using System.Security.Cryptography
@using System.IO
@using WebPassManager.Components
@using WebPassManager

@inject NavigationManager navigationManager
@inject IUsersData _db1
@inject IPasswordData _db2

<MudText Typo='Typo.h1'>Passwords Page</MudText>

<MudText Typo='Typo.h4'>Current Passwords</MudText>

<CascadingValue Value="_passwords" Name="passwords">
    <TableComponent />
</CascadingValue>


@code {
    private List<PasswordModel> _passwords;
    protected override async Task OnInitializedAsync()
    {
        _passwords = await  _db2.GetPasswords();
    }
}

        